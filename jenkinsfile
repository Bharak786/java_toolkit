pipeline {
    agent any
    
stages {

 stage ('Build Docker Image'){
        steps {
          sh '''
          cd ${WORKSPACE}
          docker build --build-arg JAVA_VERSION=${JAVA_VERSION} -t repository.nobroker.in:9094/devops/baseimage-java:${JAVA_VERSION} --file=Dockerfile ${WORKSPACE}
          '''
        }
      }
     }
    } 
